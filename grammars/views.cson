'scopeName': 'source.views'
'name': 'Views'
'fileTypes': ['view', 'data']
'patterns': [
  # block name
  {
    'match': '^([A-Z][a-zA-Z0-9]*\\s*)$'
    'name': 'views.block.keyword'
  }
  {
    'match': '^([A-Z][a-zA-Z0-9]*)\\s+[a-z]*\\s*.+$'
    'captures':
      '1':
        'name': 'views.block.keyword'
  }
  # comment
  {
    'match': '^#.+$'
    'name': 'views.comment'
  }
  # margin
  {
    'match': '^margin[a-zA-Z]*\\s*(.+)$'
    'captures':
      '1':
        'name': 'views.margin.number'
  }
  # padding
  {
    'match': '^padding[a-zA-Z]*\\s*(.+)$'
    'captures':
      '1':
        'name': 'views.padding.number'
  }
  {
    'begin': '^[a-z][a-zA-Z0-9]*\\s*'
    'end': '$'
    'patterns': [
      # props
      {
        'match': '.*(props|item).*'
        'captures':
          '0':
            'name': 'views.code.function'
      }
      # data
      {
        'match': '.*.data*'
        'captures':
          '0':
            'name': 'views.code.function'
      }
      {
        'match': '.+$'
        'name': 'views.value.string'
      }
    ]
  }
]
